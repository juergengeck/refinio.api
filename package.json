{
  "name": "@refinio/api",
  "version": "0.1.0",
  "description": "Instance-based API server for ONE platform with QUICVC transport",
  "repository": {
    "type": "git",
    "url": "https://github.com/juergengeck/refinio-packages.git",
    "directory": "refinio.api"
  },
  "author": "Juergen Geck",
  "license": "MIT",
  "keywords": [
    "refinio",
    "one-platform",
    "api",
    "quic",
    "instance"
  ],
  "type": "module",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js"
    },
    "./plan-system": {
      "types": "./dist/plan-system-index.d.ts",
      "import": "./dist/plan-system-index.js"
    },
    "./stories": {
      "types": "./dist/plan-system-index.d.ts",
      "import": "./dist/plan-system-index.js"
    },
    "./dist/state/index.js": {
      "types": "./dist/state/index.d.ts",
      "import": "./dist/state/index.js"
    },
    "./lib/state/index.js": {
      "types": "./dist/state/index.d.ts",
      "import": "./dist/state/index.js"
    }
  },
  "scripts": {
    "prebuild": "rm -rf dist",
    "build": "tsc",
    "dev": "tsc --watch",
    "start": "node dist/index.js",
    "test": "jest"
  },
  "peerDependencies": {
    "@refinio/one.core": "*",
    "@refinio/one.models": "*"
  },
  "devDependencies": {
    "@refinio/one.core": "file:../one.core",
    "@refinio/one.models": "file:../one.models",
    "@types/jest": "^30.0.0",
    "@types/node": "^20.10.0",
    "jest": "^29.7.0",
    "ts-jest": "^29.4.5",
    "typescript": "^5.3.0",
    "ws": "^8.14.2"
  },
  "engines": {
    "node": ">=18.0.0"
  }
